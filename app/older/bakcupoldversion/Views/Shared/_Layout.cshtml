<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <title>@ViewBag.Title - Mincloud</title>

    @{ Culture = UICulture = "es-CO";}

    @Styles.Render("~/Content/css")
    

    @Styles.Render("~/Content/menu")

    @Styles.Render("~/style/mincloud")
    @RenderSection("styles", required: false)


    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @*<script src="/assets/js/jquery-ui.custom.js"></script>
        <script src="/assets/js/jquery.ui.touch-punch.js"></script>*@
    @Scripts.Render("~/bundles/kendo")
    @*@Scripts.Render("~/bundles/menu")*@
    @Scripts.Render("~/js/mincoud")

    



    @*<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.0/css/bootstrap-toggle.min.css" rel="stylesheet">*@
    @*<script src="https://gitcdn.github.io/bootstrap-toggle/2.2.0/js/bootstrap-toggle.min.js"></script>*@

    @*<script src="@Url.Content("~/Scripts/kendo/2014.3.1411/jszip.min.js")"></script>*@

    <style type="text/css">
        #loaderDiv {
            position: absolute;
            width: 100%;
            height: 100%;
            text-align: center;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            z-index: 1100;
            background-color: white;
            opacity: .8;
        }


        .ajax-loader {
            position: absolute;
            top: 50%;
            left: 50%;
            margin-top: -25px;
            margin-left: -25px;
        }
    </style>
</head>
<body class="no-skin">
    <div id="loaderDiv" loading>
        <img src="~/assets/img/loading.gif" class="ajax-loader" />
    </div>
    @{Html.RenderPartial("_Header");}

    <div class="main-container ace-save-state" id="main-container">
        <script type="text/javascript">
            try {
                ace.settings.loadState('main-container');
            } catch (e) {
            }
        </script>
        @{Html.RenderPartial("_Menu");}


        <div class="main-content">
            <div class="main-content-inner">
                <div class="page-content">
                    <div class="row">
                        <div class="col-xs-12">
                            @{ Html.RenderPartial("_TopMenu"); }
                            @{ Html.RenderPartial("_Alerts"); }
                            @RenderBody()
                            

                            <hr />
                            <footer>
                                <p>&copy; @DateTime.Now.Year - Mincloud. Todos los derechos reservados</p>
                            </footer>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

<link rel="stylesheet" href="../assets/css/ace.onpage-help.css" />
<link rel="stylesheet" href="../docs/assets/js/themes/sunburst.css" />

<script type="text/javascript"> ace.vars['base'] = '..'; </script>
<script src="/assets/js/ace/elements.onpage-help.js"></script>
<script src="/assets/js/ace/ace.onpage-help.js"></script>



<script type="text/javascript">


    jQuery(function ($) {
        $('#sidebar2').insertBefore('.page-content');

        $('.navbar-toggle[data-target="#sidebar2"]').insertAfter('#menu-toggler');


        $(document).on('settings.ace.two_menu', function (e, event_name, event_val) {
            if (event_name == 'sidebar_fixed') {
                if ($('#sidebar').hasClass('sidebar-fixed')) {
                    $('#sidebar2').addClass('sidebar-fixed');
                    $('#navbar').addClass('h-navbar');
                } else {
                    $('#sidebar2').removeClass('sidebar-fixed')
                    $('#navbar').removeClass('h-navbar');
                }
            }
        }).triggerHandler('settings.ace.two_menu', ['sidebar_fixed', $('#sidebar').hasClass('sidebar-fixed')]);
    });

    $(document).ready(function () {
        //$('[data-toggle="tooltip"]').tooltip();
        // Hide the "busy" Gif at load:
        $("#loaderDiv").hide();

        $("form").on("submit", function (event) {
            //event.preventDefault();

            // Show the "busy" Gif:
            $("#loaderDiv").show();
            
        });
    });
</script>
