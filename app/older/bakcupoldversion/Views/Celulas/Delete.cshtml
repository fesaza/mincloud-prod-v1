@model ICC.Mincloud.Modelo.Entities.Celula

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-header">
    <h1>
        <i class="menu-icon fa fa-heartbeat"></i> Células
        <small>
            <i class="ace-icon fa fa-angle-double-right"></i>
            Eliminar célula
        </small>
    </h1>
</div>

<h3>Esta seguro que desea eliminar este registro?</h3>
<div>
    
    <dl class="dl-horizontal">
       


        <dt>
            @Html.DisplayNameFor(model => model.Direccion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Direccion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Telefono)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.FechaBaja)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FechaBaja)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            <a href="@Url.Action("Index")" class="btn btn-sm btn-default"><i class="fa fa-arrow-left"></i> Cancelar</a>
        </div>
    }
</div>
